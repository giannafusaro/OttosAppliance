<!-- <h1> Otto's Appliance Service Inc. </h1> -->


<div class="splash-container">
  <div class="splash">
    <h1 class="splash-head">Otto's Appliance Service Inc.</h1>
    <p class="splash-subhead subhead">
      Serving prompt, reliable appliance repairs since 1957.
    </p>
  </div>
</div>


<div class="content-wrapper home">
  <div class="content video">
    <h2 class="content-head is-center">Welcome.</h2>
    <div class="pure-g">
      <div class="l-box pure-u-3-5">
          <div class="video-home">
            <!-- 16:9 aspect ratio -->
            <video id="example_video_1" class="video-js vjs-default-skin "
              controls preload="auto" width="640" height="264"
              poster="https://fbcdn-sphotos-h-a.akamaihd.net/hphotos-ak-xap1/v/t1.0-9/10687014_10202979866566505_2099718327852753199_n.jpg?oh=e0c4dfe8d6d2d6e1e2b93dd69f180892&oe=54F38B3E&__gda__=1421203563_cbd1816d9bb997a9e7312fd87e576f9f"
              data-setup='{"example_option":true}'>
              <source src="http://cf.c.ooyala.com/J1bjI0cTpletnaKA7_rZR2iUsIWpTyEg/DOcJ-FxaFrRg4gtDEwOjEzYzowazumG4?_=daocht73nmi" type='video/mp4' />
              <p class="vjs-no-js">To view this video please enable JavaScript, and consider upgrading to a web browser that <a href="http://videojs.com/html5-video-support/" target="_blank">supports HTML5 video</a></p>
            </video>
          </div>
      </div>
      <div class="r-box pure-u-1-3">
        <div class="ottos-info">

          <h2 class="content-subhead">Otto's Appliance Service</h2>

          <div class="hours is-center">
            <strong>Hours</strong>: 8am - 5pm, Monday - Friday
            (Closed 12-1pm)
          </div>
          <table class="pure-table pure-table contact-info">
            <tbody>
              <tr>
                <td class="contact-td"><i class="fa fa-phone fa-2x"></i></td>
                <td class="contact-td"><strong> 650-588-0309 </strong></td>
              </tr>
              <tr>
                <td class="contact-td"><i class="fa fa-envelope fa-2x"></i></td>
                <td class="contact-td"><strong> ottosappliance@yahoo.com </strong></td>
              </tr>
              <tr>
                <td class="contact-td"><i id="map-marker" class="fa fa-map-marker fa-2x"></i></td>
                <td class="contact-td"><strong> 1663 El Camino Real, Millbrae, CA 94030 </strong></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <section class="ribbon l-box-lrg pure-g brand-images">

    <h2 class="content-head content-head-ribbon brand-title">Brands We Service</h2>

    <div class="pure-u-1-4 is-center">
      <%= image_tag  "Thermador.png", class: "pure-img-responsive brand" %>
    </div>

    <div class="pure-u-1-4 is-center">
      <%= image_tag  "Bosch.png", class: "pure-img-responsive brand" %>
    </div>

    <div class="pure-u-1-4 is-center">
      <%= image_tag  "Insinkerator.png", class: "pure-img-responsive brand" %>
    </div>
    <div class="pure-u-1-4 is-center">
      <%= image_tag  "Kitchenaid.png", class: "pure-img-responsive brand" %>
    </div>
    <div class="pure-u-1-4 is-center">
      <%= image_tag  "Maytag.png", class: "pure-img-responsive brand" %>
    </div>
    <div class="pure-u-1-4 is-center">
      <%= image_tag  "GE_Appliances.png", class: "pure-img-responsive brand" %>
    </div>
    <div class="pure-u-1-4 is-center">
      <%= image_tag  "Kenmore.png", class: "pure-img-responsive brand" %>
    </div>
    <div class="pure-u-1-4 is-center">
      <%= image_tag  "Whirlpool.png", class: "pure-img-responsive brand" %>
    </div>

  </section>
  <div class="footer l-box is-center">
    &copy;<%= Time.now.year %> Otto's Appliance, Inc. All rights reserved.
  </div>
</div>
